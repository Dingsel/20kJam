{
  "namespace": "pm_coins",

  "render": {
    "type": "stack_panel",
    "anchor_from": "bottom_middle",
    "orientation": "vertical",
    "size": [90, "100%c"],
    "offset": [-140, 0],
    "anchor_to": "bottom_middle",
    "factory": {
      "name": "chat_item_factory",
      "control_ids": {
        "chat_item": "coin_handle@pm_coins.coin_handle"
      }
    }
  },
  "coin_update_anim1": {
    "anim_type": "size",
    "easing": "in_cubic",
    "to": [13, 13],
    "from": [10, 10],
    "duration": 0.2,
    "next": "@pm_coins.coin_update_anim2"
  },
  "coin_update_anim2": {
    "anim_type": "size",
    "easing": "out_cubic",
    "to": [10, 10],
    "from": [13, 13],
    "duration": 0.2
  },
  "coin_update@rt20k.img": {
    "texture": "textures/ui/hud_tip_text_background",
    "alpha": 0.4,
    "bindings": [
      {
        "binding_name": "#chat_text",
        "binding_name_override": "#text",
        "binding_type": "collection",
        "binding_collection_name": "chat_text_grid",
        "binding_condition": "once"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.19s' * #text)",
        "target_property_name": "#prefix"
      },
      {
        "binding_type": "view",
        "source_property_name": "(not((#prefix - 'RTKJAM:coins_update') = #prefix))",
        "target_property_name": "#visible"
      }
    ],
    "anchor_from": "bottom_right",
    "offset": [0, -5.5],
    "anchor_to": "bottom_right",
    "size": ["100%c+6px", "100%c+4px"],
    "controls": [
      {
        "stack@rt20k.sp": {
          "orientation": "horizontal",
          "size": ["100%c", 10],
          "controls": [
            {
              "text_p@rt20k.p": {
                "size": ["100%c", 10],
                "controls": [
                  {
                    "text@rt20k.l": {
                      "layer": 1,
                      "bindings": [
                        {
                          "binding_name": "#chat_text",
                          "binding_name_override": "#text",
                          "binding_type": "collection",
                          "binding_collection_name": "chat_text_grid",
                          "binding_condition": "once"
                        },
                        {
                          "binding_type": "view",
                          "source_property_name": "(('ยง6' + #text) - 'RTKJAM:coins_update')",
                          "target_property_name": "#final"
                        }
                      ],
                      "offset": [0, -0.5],
                      "shadow": true,
                      "text": "#final"
                    }
                  }
                ]
              }
            },
            {
              "pad@rt20k.p": {
                "size": [2, 10]
              }
            },
            {
              "img@rt20k.p": {
                "size": ["100%c", 10],
                "controls": [
                  {
                    "img@rt20k.img": {
                      "anims": ["@pm_coins.coin_update_anim1"],
                      "texture": "rt20k/coin",
                      "size": [10, 10]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },
