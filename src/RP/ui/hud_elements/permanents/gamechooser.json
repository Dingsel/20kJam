{
  "namespace": "pm_gamechooser",

  "render": {
    "type": "stack_panel",
    "anchor_from": "bottom_middle",
    "orientation": "vertical",
    "size": [180, "100%c"],
    "offset": [0, "-50%"],
    "anchor_to": "bottom_middle",
    "factory": {
      "name": "chat_item_factory",
      "control_ids": {
        "chat_item": "handle@pm_gamechooser.handle"
      }
    }
  },

  "handle@rt20k.p": {
    "size": ["100%c", "100%c"],
    "controls": [
      {
        "size_handle@rt20k.p": {
          "size": [180, 1000],
          "bindings": [
            {
              "binding_name": "#chat_text",
              "binding_name_override": "#text",
              "binding_type": "collection",
              "binding_collection_name": "chat_text_grid",
              "binding_condition": "once"
            },
            {
              "binding_type": "view",
              "source_property_name": "('%.13s' * #text)",
              "target_property_name": "#prefix"
            },
            {
              "binding_type": "view",
              "source_property_name": "(not((#prefix - 'RTKJAM:cgame') = #prefix))",
              "target_property_name": "#visible"
            }
          ],
          "controls": [
            {
              "center_panel@rt20k.p": {
                "anchor_from": "bottom_middle",
                "anims": ["@pm_gamechooser.center_panel_anim_1"],
                "anchor_to": "bottom_middle",
                "size": [180, 0],
                "controls": [
                  {
                    "choosing_game_text@pm_gamechooser.choosing_game_text": {
                        "anims": ["@pm_gamechooser.choosing_game_anim_1_1"],
                      "anchor_from": "center",
                      "anchor_to": "center"
                    }
                  },
                  {
                    "gamestack": {
                      "type": "panel",
                      "size": [180, 18],
                      "clips_children": true,
                      "anchor_from": "top_middle",
                      "anchor_to": "top_middle",
                      "controls": [
                        {
                          "stack1@pm_gamechooser.gamemodes_stack1": {
                            "offset": [0,0],
                            "layer": 2,
                            "anchor_from": "top_middle",
                            "anchor_to": "top_middle"
                          }
                        },
                        {
                            "background@rt20k.img": {
                                    "size": [0,0],
                                    "texture": "textures/ui/hud_tip_text_background",
                                    "alpha": 0.3,
                                    "anims": ["@pm_gamechooser.background_anim_1_1"]
                            }
                        }
                      ]
                    }
                  }
                ]
              }
            },
            {
                "white@rt20k.img": {
                    "texture": "rt20k/white",
                    "alpha": 0,
                    "layer": 1000,
                    "anims": ["@pm_gamechooser.white_1_1"],
                    "size": [2000,2000]
                }
            },
            {
                "black@rt20k.img": {
                    "alpha": 0,
                    "layer": 1100,
                    "anims": ["@pm_gamechooser.black_1_1"],
                    "size": [2000,2000]
                }
            }
          ]
        }
      }
    ]
  },

"center_panel_anim_1": {
    "anim_type": "wait",
    "duration": 6.9,
    "next": "@pm_gamechooser.center_panel_anim_2"
},

"center_panel_anim_2": {
    "anim_type": "offset",
    "duration":0.2,
    "from": [0,0],
